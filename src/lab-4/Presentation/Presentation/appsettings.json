{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Grpc": "Debug"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000",
        "Protocols": "Http2"
      }
    }
  },
  "DbConnection": {
    "Host": "localhost",
    "Port": "5433",
    "Username": "user",
    "Password": "pass",
    "Database": "lab3_test"
  },
  "ServiceConnection": {
    "Settings": {
      "ConnectionHost": "localhost",
